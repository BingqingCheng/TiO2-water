# Energy - kJ/mol.
UNITS LENGTH=A TIME=ps
# RESTART

DISTANCES ...
LABEL=d
GROUPA=OOOOO 
GROUPB=HHHHH1-MMMMM:3,HHHHH2-MMMMM:3
MIN={BETA=100.} 
... DISTANCES

METAD ...
LABEL=metad
ARG=d.min
PACE=500 HEIGHT=0.2 SIGMA=200 FILE=HILLS
TEMP=300 BIASFACTOR=20
ADAPTIVE=DIFF SIGMA_MAX=1 SIGMA_MIN=0.001
... METAD

#DUMPMULTICOLVAR DATA=m3 FILE=m3.xyz STRIDE=1000

uwall: UPPER_WALLS ARG=d.min AT=2.5 KAPPA=100 EXP=2

# monitor the two variables and the metadynamics bias potential
PRINT STRIDE=10 ARG=d.*,metad.bias,uwall.bias  FILE=COLVAR

ENDPLUMED

